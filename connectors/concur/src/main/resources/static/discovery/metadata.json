{
  "image": {
    "href": "${CONNECTOR_HOST}/images/connector.png"
  },
  "test_auth": {
    "href": "${CONNECTOR_HOST}/test-auth"
  },
  "object_types": [
    {
      "name": "card",
      "doc": {
        "href": "https://github.com/vmwaresamples/card-connectors-guide/wiki/Card-Responses"
      },
      "fields": {
        "concur_automated_email_subject": {
          "regex": "Expense Report Pending Your Approval"
        },
        "expense_report_id": {
          "capture_group": 1,
          "regex": "\\b([A-F0-9]{20,})\\b"
        }
      },
      "endpoint": {
        "href": "${CONNECTOR_HOST}/cards/requests"
      }
    }
  ]
}
