{
  "authorization_header": "x-salesforce-authorization",
  "backend_base_url_header": "x-salesforce-base-url",
  "object_types": [
    {
      "object_type": {
        "name": "card",
        "doc": {
          "href": "https://github.com/vmwaresamples/card-connectors-guide/wiki/Card-Responses"
        }
      },
      "fields": {
        "user_email": {
          "env": "USER_EMAIL"
        },
        "sender_email": {
          "capture_group": 1,
          "regex": "([A-Z0-9a-z._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,6})"
        }
      },
      "endpoint": {
        "href": "CONNECTOR_HOST/cards/requests"
      }
    }
  ]
}
